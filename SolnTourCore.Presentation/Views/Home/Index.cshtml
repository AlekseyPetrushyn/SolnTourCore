@model IEnumerable<SolnTourCore.Presentation.Models.HotelCategory>
@{
	ViewData["Title"] = "Home Page";
}


<table>
	<tr>
		<td>id</td>
		<td>Name</td>
		<td>Description</td>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.HotelCategoryId</td>
			<td>@item.HotelCategoryName</td>
			<td>@item.Description</td>
		</tr>

	}
</table>

