@model IEnumerable<SolnTourCore.DataAccess.Entities.Hotel>
@{
	ViewData["Title"] = "Home Page";
}


<table>

	@foreach (var item in Model)
	{
		<tr>
			<td>@Html.DisplayFor(modelItem => item.HotelId)</td>
			<td>@Html.DisplayFor(modelItem => item.HotelName)</td>
		    <td>@Html.DisplayFor(modelItem => item.Place.PlaceName)</td>
		    <td>@Html.DisplayFor(modelItem => item.Place.Country.CountryName)</td>
		    <td>@Html.DisplayFor(modelItem => item.Accomodation.AccomodationName)</td>
            <td>@Html.DisplayFor(modelItem => item.Price)</td>

		</tr>

	}
</table>

