@model IEnumerable<SolnTourCore.Presentation.Models.Accomodation>
@{
	ViewData["Title"] = "Home Page";
}


<table>
	<tr>
		<td>id</td>
		<td>Name</td>
		<td>Description</td>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.AccomodationId</td>
			<td>@item.AccomodationName</td>
			<td>@item.Description</td>
		</tr>

	}
</table>

